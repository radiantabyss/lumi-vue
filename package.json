{
    "name": "lumi-vue",
    "version": "1.0.0",
    "private": true,
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "globals": {
            "Alert": "writable",
            "Cookie": "writable",
            "Item": "writable",
            "Items": "writable",
            "Request": "writable",
            "StorageHandler": "writable"
        }
    }
}
